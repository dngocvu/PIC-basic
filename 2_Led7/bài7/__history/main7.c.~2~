#include <main7.h>

const unsigned int8 ma7doan[10] = {0x180,0x1f2,0x148,0x160,0x132,0x124,0x104,0x1f0,0x100,0x120};
signed int dem;
unsigned int ma_dem;
void main()
{
   set_tris_c(0x00);
   set_tris_d(0x00); // input port d
   while(TRUE)
   {
      for(dem=0;dem<10;dem++)
         {
            //ma_dem = ma7doan[dem];  
            output_c(ma7doan[dem]);
            output_d(ma7doan[dem]);
            delay_ms(50);
         }

      //TODO: User Code
   }

}
